    "@paciolan/tooltip": "^0.1.8",
    "@paciolan/utilities": "*",
    "@paciolan/virtualized-table": "^0.2.21",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.4",
    "lodash.orderby": "^4.6.0",
    "material-ui": "^1.0.0-beta.36",
    "papaparse": "^4.6.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^5.0.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
