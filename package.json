    "reselect": "^3.0.1",
    "styled-components": "^3.4.5",
    "svg-loader": "0.0.2",
    "tinycolor2": "^1.4.1",
    "whatwg-fetch": "^2.0.4",
    "why-did-you-update": "^0.1.1"
  },
  "devDependencies": {
    "@paciolan/dynamic-loader": "^1.1.1",
